{
  "name": "kin-cli",
  "description": "A flexible template-based project generator built with TypeScript",
  "version": "0.0.2",
  "author": "David Myers <david@voracious.dev>",
  "bugs": {
    "url": "https://github.com/voraciousdev/kin-cli/issues"
  },
  "funding": "https://github.com/sponsors/voraciousdev",
  "homepage": "https://github.com/voraciousdev/kin-cli",
  "keywords": [
    "boilerplate",
    "cli",
    "generator",
    "interactive",
    "scaffold",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voraciousdev/kin-cli.git"
  },
  "bin": {
    "kin": "./kin.js"
  },
  "dependencies": {
    "cac": "^6.7.12",
    "inquirer": "^9.0.1",
    "liquidjs": "^9.39.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.4",
    "@yarnpkg/esbuild-plugin-pnp": "^3.0.0-rc.12",
    "chalk": "^5.0.1",
    "esbuild": "^0.14.49",
    "execa": "^6.1.0",
    "makke": "workspace:^",
    "rimraf": "^3.0.2",
    "string-argv": "^0.3.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "packageManager": "yarn@3.2.1",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "rimraf ./dist && makke dev",
    "build": "rimraf ./dist && makke build",
    "makke:build": "yarn workspace makke build",
    "makke:publish": "yarn workspace makke npm publish"
  },
  "type": "module",
  "workspaces": [
    ".",
    "./lib/makke"
  ]
}
